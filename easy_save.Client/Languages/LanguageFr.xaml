<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <system:String x:Key="French">Français</system:String>
    <system:String x:Key="English">Anglais</system:String>

    <system:String x:Key="Refresh">Rafraichir</system:String>
    <system:String x:Key="Remove">Supprimer</system:String>
    <system:String x:Key="Launch">Lancer</system:String>

    <system:String x:Key="RemoveThread">Supprimer thread</system:String>
    <system:String x:Key="PauseThread">Mettre en pause thread</system:String>
    <system:String x:Key="ResumeThread">Relancer thread</system:String>

</ResourceDictionary>